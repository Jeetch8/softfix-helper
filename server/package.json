{
    "name": "tts-converter",
    "version": "1.0.0",
    "description": "Convert text to speech using Gemini AI API and save as MP3",
    "type": "module",
    "main": "server.js",
    "bin": {
        "tts-converter": "tts-converter.js"
    },
    "scripts": {
        "start": "node server.js",
        "dev": "node server.js",
        "test": "node tts-converter.js 'Hello, this is a test' -o test-output.mp3",
        "import-keywords": "node scripts/import-csv-keywords.js"
    },
    "keywords": [
        "text-to-speech",
        "tts",
        "gemini",
        "ai",
        "mp3",
        "audio",
        "youtube",
        "narration"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@google/genai": "^1.39.0",
        "@google/generative-ai": "^0.21.0",
        "aws-sdk": "^2.1693.0",
        "cors": "^2.8.6",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "fluent-ffmpeg": "^2.1.2",
        "mongoose": "^7.0.0",
        "multer": "^2.0.2",
        "node-cron": "^3.0.2",
        "wav": "^1.0.2",
        "xlsx": "^0.18.5"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}